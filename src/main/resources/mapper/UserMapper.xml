<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.babymeal.user.UserMapper">
    <update id="delUser" useGeneratedKeys="true" keyProperty="iuser">
        update user
        set del_yn = '1'
        where uid = #{uid}
    </update>
</mapper>